<section class="hg_section pt-80 pb-80 page" style="background-color: rgba(240, 238, 230, 0.91);" id="nguoidung">
		<div class="container">
			<div class="row">
				<div class="col-sm-12 col-md-12">
					<!-- Title element -->
					<div class="kl-title-block text-center">
						<!-- Title with custom font size and thin style -->
						<h3 class="tbk__title fs-xxl fw-thin">
							<span class="fw-semibold fs-xxxl fs-xs-xl tcolor" style="text-align:center; color:rgba(255,0,0,0.65);margin-top:100px;font-weight:400;font-family: 'Itim', cursive;
							font-family: 'Arima Madurai', cursive; font-size:35px;">
								Trải nghiệm của khách hàng đã và đang dùng Putier 5th
						</h3>
						<!--/ Title with custom font size and thin style -->

						<!-- Spacer with custom height -->
						<div style="height: 50px;"></div>
					</div>
					<!--/ Title element -->
				</div>
				<!--/ col-sm-12 col-md-12 -->

				<% data.experience.forEach(exp => { %>

				<div class="col-sm-12 col-md-12 col-lg-4" style=" font-family: 'Comfortaa', cursive;">
					<!-- Title element centered -->
					<div class="kl-title-block text-center">
						<!-- Title with custom font, size and weight -->
						<h3 class="tbk__title kl-font-alt fs-m " style=" font-size:1rem;">
							<%= exp.title%>							
						</h3>
						<!--/ Title with custom font and size -->
					</div>
					<!--/ Title element centered -->

					<!-- Video box element iframe style -->
					<div class="fluid-width-video-wrapper" style="padding-top: 75%;">
						<iframe src="https://www.youtube.com/embed/<%= exp.link%>?rel=0&amp;controls=0&amp;showinfo=0" frameborder="0" allowfullscreen=""></iframe>
					</div>
					<!--/ .fluid-width-video-wrapper -->
				</div>

				<% }) %>
			</div>
			<!--/ row -->
		</div>
		<!--/ container -->
	</section>