<section class="hg_section pt-80 pb-80">
		<div class="container">
			<div class="row">
				<div class="col-sm-12 col-md-12">
					<!-- Title element -->
					<div class="kl-title-block text-center tbk-symbol-- tbk-icon-pos--after-title">
						<!-- Title with bold style -->
						<h3 class="fw-bold" style="text-align:center; color:rgba(255,0,0,0.65);margin-top:100px;font-weight:400;font-family: 'Itim', cursive;
						font-family: 'Arima Madurai', cursive; font-size:48px;">
							Đối tượng sử dụng
						</h3>
						<!--/ Title with bold style -->
					</div>
					<!--/ Title element -->

					<!-- Historic timeline element -->
					<div class="timeline_bar" style=" font-family: 'Comfortaa', cursive;">
						<div class="row">
						<% 
						var countObj = 1;
						data.user_manual.forEach(um => { 
							if(countObj % 2 != 0)
							{
						%> 
							<!-- Timeline box 1 -->
							<div class="col-sm-12 col-md-6 offset-md-6" data-align="right">
								<!-- Timeline box -->
								<div class="timeline_box">
									<!-- Date -->
									<div class="date">
										<h1><%= countObj%></h1>
									</div>
									<!--/ Date -->

									<!-- Title -->

									<!--/ Title -->

									<!-- Content -->
									<p>
											<%= um.content%>
									</p>
									<!--/ Content -->
								</div>
								<!--/ Timeline box -->
							</div>
							<!--/ Timeline box 1 -->
						<%
							countObj++;} else {
						%>
							<!-- Timeline box 2 -->
							<div class="col-sm-12 col-md-6">
								<!-- Timeline box -->
								<div class="timeline_box">
									<!-- Date -->
									<div class="date">
										<h1><%=countObj%></h1>
									</div>
									<!-- Content -->
									<p>
										<%=um.content%>
									</p>
									<!-- Content -->
								</div>
								<!--/ Timeline box -->
							</div>
							<!--/ Timeline box 2 -->
						<% 
							countObj++;
							}
						}) 
						%>	
						</div>
						<!--/ row -->
					</div>
					<!--/ Historic timeline element -->
					<div class="kl-title-block text-center tbk-symbol-- tbk-icon-pos--after-title" style="padding-top:40px;">
						<!-- Title with bold style -->
						<h3 class="fw-bold" style="text-align:center; color:rgba(255,0,0,0.65);margin-top:100px;font-weight:400;font-family: 'Itim', cursive;
						font-family: 'Arima Madurai', cursive; font-size:48px;">
							Liều Dùng
						</h3>
						<!--/ Title with bold style -->

						<!-- Sub-title with thin style -->
						<% data.dosage.forEach(dos => { %>  
						<h4 class="tbk__subtitle tbk-thinsubtitle">
							<%= dos.content%>
						</h4>
						<%})%>
						<!--/ Subtitle with thin style -->
					</div>
				</div>
				<!--/ col-sm-12 col-md-12 -->
			</div>
			<!--/ row -->
		</div>
		<!--/ container -->
	</section>