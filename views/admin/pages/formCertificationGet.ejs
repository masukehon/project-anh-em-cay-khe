<%if(messages.length !== 0){ %>
    <div style="margin: 0 0;width:100%;text-align:center;">
        <p class="alert-success" style="padding:15px 250px;font-size:18px">Notify: <%=messages%></p>
    </div>
<% } %>
<form action="/admin/certification/update" enctype="multipart/form-data" method="POST">
    <div class="form-group">
        <h3>Select multiple images</h3>
      <input type="file" name="images" class="form-control-file" multiple> 
    </div>
    <br>
    <button type="submit" class="btn btn-primary mb-2">Update</button>
</form>
<hr>
<h3 style="text-align: center">List image certification</h3>
<table class="table">
    <% var dem=1 %>
    <%cer.images.forEach(img=>{ %>
    <tr style="text-align: center">
        <td>image <%=dem%></td>
        <td><img src="https://s3-ap-southeast-1.amazonaws.com/purtier/<%=img%>" alt="" width="150px"></td>
    </tr>
    <%dem++%>
    <% }) %>
</table>