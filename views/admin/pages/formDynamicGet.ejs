<div class="container">
    <h3><a href="/admin/dynamic/<%=dynamicForm%>/insert">Insert Dynamic</a></h3>  
</div>
<hr>
<table id="datatable" class="table table-striped table-bordered">
    <thead>
        <tr>
               <th>Title</th>
               <th>Content</th>
               <th>image Primary</th>
               <th>unit</th>
               <th>image Sub</th>
               <th>highlight</th>
               <th>Delete</th>
               <th>Edit</th>
           </tr>
       </thead>
       <tbody>
            <%feats.forEach(f=>{ 
                const {_id,imagePrimary,imageSub,title,content,isHighlight,unit,category} = f
            %> 
            <tr>
               <td><%= title%></td>
               <td><%= content%></td>
               <td><%= unit%></td>
               <td>
                   <img src="images/admin/<%= imagePrimary%>" width="50px" alt="">
               </td>
               <td>
                    <img src="admin/image/<%= imageSub%>" width="50px" alt="">
               </td>
               <td>
                   <input type="checkbox">
               </td>
               <td class="center"><i class="fa fa-trash-o fa-fw"></i><a href="/admin/dynamic/remove/<%=_id%>"> Xóa</a></td>
               <td class="center"><i class="fa fa-pencil fa-fw"></i> <a href="/admin/dynamic/frUpdate/<%=_id%>">Sữa</a></td>
            </tr>
            <% })%>
        </tbody>
</table>