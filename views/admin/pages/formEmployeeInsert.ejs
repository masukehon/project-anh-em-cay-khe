<ol class="breadcrumb">
        <li class="breadcrumb-item">
          <h3><a href="admin/all" style="text-decoration: underline">List Employess</a></h3>
        </li>
        <li class="breadcrumb-item active">Add Employess</h3></li>
    </ol>

   <%if(messages.length !== 0){ %>
    <div style="margin: 0 0;width:100%;text-align:center;">
      <p class="alert-success" style="padding:15px 250px;font-size:18px">Notify: <%=messages%></p>
  </div>
  <% } %>
<div class="container">
        <hr>
        <form action="admin/signup" method="POST">
          <div class="form-group">
            <label>Email:</label>
            <input type="email" class="form-control" placeholder="Enter email" name="email">
          </div>
          <div class="form-group">
            <label>Password:</label>
            <input type="password" class="form-control" placeholder="Enter password" name="password">
          </div>
          <div class="form-group">
            <label>Name:</label>
            <input type="text" class="form-control" placeholder="Enter Name" name="name">
          </div>
          <div class="form-group">
            <label>Address:</label>
           <textarea class="form-control" name="address" cols="40" rows="3"></textarea>
          </div>
          <div class="form-group">
            <label>Phone:</label>
            <input type="text" class="form-control" placeholder="Enter phone" name="phone">
          </div>
          <button type="submit" class="btn btn-success">Add</button>
        </form>
    </div>